{
    "name": "Title Selector",
    "description": "A title selector extension",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab"],
    "action": {
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "/images/title-selector16.png",
        "32": "/images/title-selector32.png",
        "48": "/images/title-selector48.png"
    }
}